{
	"name": "velocity.report-web",
	"private": true,
	"version": "0.4.0-pre1",
	"type": "module",
	"license": "Apache-2.0",
	"scripts": {
		"dev": "node scripts/set-build-env.js dev pnpm exec vite dev",
		"build": "node scripts/set-build-env.js build pnpm exec vite build",
		"postbuild": "node scripts/copy-build.js",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"up-deps": "pnpm update -r -i --latest",
		"format": "pnpm run prettier:write",
		"prettier:check": "prettier --check .",
		"prettier:write": "prettier --write .",
		"lint": "pnpm run prettier:check && eslint .",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:ci": "jest --ci --coverage --maxWorkers=2"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.39.1",
		"@layerstack/tailwind": "2.0.0-next.19",
		"@mdi/js": "^7.4.47",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.0",
		"@sveltejs/vite-plugin-svelte": "6.2.1",
		"@tailwindcss/postcss": "^4.1.17",
		"@tailwindcss/vite": "^4.1.17",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.9",
		"@types/d3-scale": "^4.0.9",
		"@types/jest": "^30.0.0",
		"cross-env": "^10.1.0",
		"d3-scale": "4.0.2",
		"date-fns": "4.1.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"jest": "^30.2.0",
		"jest-environment-jsdom": "^30.2.0",
		"layerchart": "2.0.0-next.43",
		"prettier": "3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"svelte": "^5.45.2",
		"svelte-check": "^4.3.4",
		"svelte-jester": "^5.0.0",
		"svelte-ux": "2.0.0-next.21",
		"tailwindcss": "^4.1.17",
		"ts-jest": "^29.4.5",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.0",
		"vite": "^7.2.4"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"esbuild",
			"svelte-preprocess",
			"unrs-resolver"
		],
		"overrides": {
			"cookie@<0.7.0": ">=0.7.0",
			"devalue@<5.3.2": ">=5.3.2"
		}
	},
	"dependencies": {
		"@layerstack/utils": "2.0.0-next.16"
	}
}
