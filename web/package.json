{
	"name": "velocity.report-web",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"license": "Apache-2.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"postbuild": "rm -rf ../static/app && cp -r build/* ../static/",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"up-deps": "pnpm update -r -i --latest",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:ci": "jest --ci --coverage --maxWorkers=2"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.34.0",
		"@layerstack/tailwind": "2.0.0-next.17",
		"@mdi/js": "^7.4.47",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.36.2",
		"@sveltejs/vite-plugin-svelte": "6.1.3",
		"@tailwindcss/vite": "^4.1.11",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.6",
		"@types/d3-scale": "^4.0.9",
		"@types/jest": "^29.5.14",
		"d3-scale": "4.0.2",
		"date-fns": "2.30.0",
		"eslint": "^9.34.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"layerchart": "2.0.0-next.35",
		"svelte-jester": "^5.0.0",
		"ts-jest": "^29.2.5",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.38.3",
		"svelte-check": "^4.3.0",
		"svelte-ux": "2.0.0-next.18",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.41.0",
		"vite": "^7.1.11"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"esbuild",
			"svelte-preprocess"
		]
	},
	"dependencies": {
		"@layerstack/utils": "2.0.0-next.14"
	}
}
